riginal":{"line":163,"column":12},"generated":{"line":1,"column":2317}},{"source":"scss/globals/siteheader.scss","name":null,"original":{"line":164,"column":12},"generated":{"line":1,"column":2344}},{"source":"scss/globals/siteheader.scss","name":null,"original":{"line":165,"column":16},"generated":{"line":1,"column":2379}},{"source":"scss/globals/siteheader.scss","name":null,"original":{"line":172,"column":0},"generated":{"line":1,"column":2405}},{"source":"scss/globals/siteheader.scss","name":null,"original":{"line":173,"column":4},"generated":{"line":1,"column":2424}},{"source":"scss/globals/siteheader.scss","name":null,"original":{"line":173,"column":7},"generated":{"line":1,"column":2427}},{"source":"scss/globals/siteheader.scss","name":null,"original":{"line":174,"column":4},"generated":{"line":1,"column":2455}},{"source":"scss/globals/siteheader.scss","name":null,"original":{"line":174,"column":8},"generated":{"line":1,"column":2459}},{"source":"scss/globals/siteheader.scss","name":null,"original":{"line":175,"column":4},"generated":{"line":1,"column":2483}},{"source":"scss/globals/siteheader.scss","name":null,"original":{"line":175,"column":9},"generated":{"line":1,"column":2486}}],"sources":{"scss/globals/siteheader.scss":"@use 'colors' as c;\n@use '../abstracts/breakpoints' as *;\n\n.siteheader {\n    position: relative;\n    top: 0;\n    width: 100%;\n    .SmallHeader {\n        width: 100%;\n        box-sizing: border-box;\n        padding: 16px;\n        background: c.$bright;\n        box-shadow: c.$shadow;\n        position: sticky;\n        top: 0;\n        z-index: 2;\n        @include mq(400px) {\n            padding: 16px 26px;\n        }\n    }\n    .button.circle {\n        position: absolute;\n        top: 100%;\n        transform: translateY(-50%);\n        right: 80px;\n        z-index: 2;\n        &:last-of-type {\n            right: 20px;\n        }\n    }\n    .banner {\n        width: 100%;\n        height: 38px;\n        background-color: c.$dark;\n        color: c.$bright;\n        position: relative;\n        width: 100%;\n        box-sizing: border-box;\n        overflow: hidden;\n        z-index: 1;\n        span {\n            position: absolute;\n            top: 10px;\n            display: block;\n            animation: marquee 26s linear infinite;\n            @include mq(660px) {\n                animation: none;\n                width: 100%;\n                text-align: center;\n            }\n        }\n        &::after {\n            content: '';\n            display: block;\n            position: absolute;\n            right: 0;\n            top: 0;\n            height: 100%;\n            width: 170px;\n            background-image: linear-gradient(90deg, rgba(34,34,34,0.00) 0%, #222222 21%, #222222 100%);\n        }\n    }\n}\n\n// siteheader scroll to sticky setup\nbody.sticky {\n    .siteheader {\n        position: fixed;\n        z-index: 1;\n        .SmallHeader {\n            position: relative;\n        }\n        .banner {\n            margin: 0;\n            position: absolute;\n            top: 0;\n            z-index: -1;\n        }\n    }\n    .sitecontent {\n        // calc the size here because the update \n        // of the vars makes the scroll not smooth\n        margin-top: calc(var(--SmallHeaderHeight) + var(--BannerHeight));\n    }\n}\n.sitecontent {\n    margin-top: 0;\n}\n\n// mobile menu\n.siteheader {\n    .sitenav {\n        display: flex;\n        flex-direction: column;\n        justify-content: space-between;\n        position: fixed;\n        transform: translateY(-100vh);\n        left: 0;\n        top: var(--SiteHeaderHeight);\n        width: 100%;\n        height: calc(100vh - var(--SiteHeaderHeight));\n        transition: transform .3s ease-out;\n        background: c.$bright;\n        box-shadow: c.$shadow;\n        z-index: 1;\n        padding: 40px 0 60px 40px;\n        box-sizing: border-box;\n        ul {\n            max-width: calc(100vw - 60px);\n        } \n        li {\n            list-style-type: none;\n        }\n        > ul a {\n            display: inline-block;\n            padding: 10px 0;\n            margin-bottom: 20px;\n        }\n\n        .MetaNav {\n            hr {\n                border: 1px solid c.$dark;\n                margin: 10px 0;\n            }\n            .LegalNav {\n                ul {\n                    display: flex;\n                }\n                a {\n                    display: inline-block;\n                    margin-right: 20px;\n                    padding: 10px 0;\n                }\n            }\n\n        }\n    }\n}\nbody.menu {\n    overflow: hidden;\n    .hamburger {\n        span {\n            transform: translateY(50%) rotate(45deg);\n            &:last-of-type {\n                transform: translateY(-50%) rotate(-45deg);\n                margin: 0;\n            }\n        }        \n    }\n    .siteheader {\n        .sitenav {\n            transform: translateY(0);\n        }\n    }\n}\n.hamburger {\n    span {\n        transition: transform .2s ease-out;\n        \n    }\n    &:hover {\n        span {\n            transform: translateY(-20%);\n            &:last-of-type {\n                transform: translateY(20%);\n            }\n        }\n    }\n}\n\n// animation for the banner\n@keyframes marquee {\n    0%{transform: translateX(-100%);}\n    50%{transform: translateX(0);}\n    100%{transform: translateX(100%);}\n}","scss/globals/_colors.scss":"$bright: #fff;\n$dark: #222222;\n\n\n$shadow: 0 2px 8px 0 rgba(0,0,0,0.08);\n","scss/abstracts/breakpoints.scss":"\n@mixin mq($size) {\n    @media (min-width: $size) {\n        @content;\n    }\n}"},"lineCount":null}},"error":null,"hash":"025ff8b75ed1f8158484a9e9477b3ab4","cacheData":{"env":{}}}